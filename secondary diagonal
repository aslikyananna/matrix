ls = [[5, 6, 9],
    [26, 12, 56],
    [10, 13, 65]]
for index in range(len(ls)*len(ls[0])):
     i = index//len(ls)
     j = index % len(ls)
     if i + j == len(ls)-1:
         print(ls[i][j])
///
ls = [[5, 6, 9],
    [26, 12, 56],
    [10, 13, 65]]
new_ls =[]
for index in range(len(ls)*len(ls[0])):
     i = index//len(ls)
     j = index % len(ls)
     if i + j == len(ls)-1:
         new_ls.append(ls[i][j])
print(new_ls)
